{
    "name": "apolloweb/wp-woocommerce-printify-sync",
    "description": "Sync products from Printify to WooCommerce",
    "type": "wordpress-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ApolloWeb\\WPWooCommercePrintifySync\\": "src/"
        }
    },
    "require": {
        "php": ">=7.3",
        "eftec/bladeone": "^4.9",
        "guzzlehttp/guzzle": "^7.0",
        "phpseclib/phpseclib": "~3.0",
        "ext-json": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.0",
        "wp-coding-standards/wpcs": "^2.3"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}