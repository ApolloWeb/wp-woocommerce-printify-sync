{
    "name": "apolloweb/wp-woocommerce-printify-sync",
    "description": "WooCommerce Printify Sync Plugin",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=8.2",
        "vlucas/phpdotenv": "^5.6",
        "composer/installers": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "ApolloWeb\\WooCommercePrintifySync\\": "wp-content/plugins/wp-woocommerce-printify-sync/src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}