{
  "name": "myvendor/my-wordpress-project",
  "description": "A WordPress development environment.",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "johnpbloch/wordpress-core-installer": "^2.0",
    "johnpbloch/wordpress": "^6.3",
    "apolloweb/wp-woocommerce-printify-sync": "dev-main",
    "wpackagist-theme/envo-shopper": "^1.0",
    "wpackagist-plugin/wordpress-seo": "^21.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ApolloWeb/wp-woocommerce-printify-sync"
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "johnpbloch/wordpress-core-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "MyPlugin\": "plugins/wp-woocommerce-printify-sync/"
    }
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
      "./plugins/{$name}/": ["type:wordpress-plugin"],
      "./themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}