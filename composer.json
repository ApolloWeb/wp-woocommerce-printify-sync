{
  "name": "apolloweb/wp-woocommerce-printify-sync",
  "description": "WordPress plugin to provide syncing between Woocommerce and Printify.",
  "type": "project",
  "license": "proprietary",
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "phpstan/phpstan": "^1.10",
    "wp-coding-standards/wpcs": "^2.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "szepeviktor/phpstan-wordpress": "^1.3",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "overtrue/phplint": "^9.0.0",
    "woocommerce/woocommerce-sniffs": "^0.1.0",
    "phpcsstandards/phpcsutils": "v1.0.12",
    "phpcsstandards/phpcsextra": "v1.2.1"
  },
  "scripts": {
    "phplint": "./vendor/bin/parallel-lint --exclude .git --exclude vendor .",
    "phpcs": "./vendor/bin/phpcs --runtime-set ignore_warnings_on_exit 1 --runtime-set ignore_errors_on_exit 1",
    "phpcbf": "./vendor/bin/phpcbf -q --standard=WordPress --report=summary wp-content/plugins/wp-woocommerce-printify-sync",
    "phpstan": "./vendor/bin/phpstan analyse --error-format=table --no-progress",
    "check": [
      "@phplint",
      "@phpcs",
      "@phpstan"
    ],
    "fix": "@phpcbf"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "platform": {
      "php": "8.1"
    }
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/phpcsstandards/phpcsutils"
    },
    {
      "type": "vcs",
      "url": "https://github.com/phpcsstandards/phpcsextra"
    }
  ]
}